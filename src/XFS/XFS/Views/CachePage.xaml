<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="XFS.Views.CachePage">
    <StackLayout>
        <Label Text="Nome"/>
        <Entry Text="{Binding Nome}"/>
        <Label Text="{Binding LabelNome}"/>
        <Button Command="{Binding AdicionarCacheCommand}" Text="Adicionar cache"/>
        <Button Command="{Binding RemoverCacheCommand}" Text="Remover cache"/>
    </StackLayout>
</ContentPage>